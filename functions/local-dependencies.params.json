{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "organizationName": {
        "value": "abc"
      },
      "applicationName": {
        "value": "appname"
      },
      "hostName": {
        "value": "local"
      },
      "disableKeyVault": {
        "value": true
      },
      "serviceBusQueues": {
        "value": [
          "queue1",
          "queue2"
        ]
      },
      "storageAccounts": {
        "value": [
          {
            "suffix": "1",
            "comment": "Storage for files",
            "containers": [
              "container1",
              "container2"
            ],
            "readOnly": true,
            "daysBeforeDeletion": 365,
            "allowBlobPublicAccess": false
          }
        ]
      },
      "useCosmosAccount": {
        "value": "true"
      }
    }
  }