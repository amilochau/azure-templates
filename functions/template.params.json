{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "organizationName": {
        "value": "abc"
      },
      "applicationName": {
        "value": "appname"
      },
      "hostName": {
        "value": "dev"
      },
      "pricingPlan": {
        "value": "Free"
      },
      "applicationType": {
        "value": "isolatedDotnet6"
      },
      "serviceBusQueues": {
        "value": [
          "queue1",
          "queue2"
        ]
      },
      "storageAccounts": {
        "value": [
          {
            "suffix": "1",
            "comment": "Storage for files",
            "containers": [
              "container1",
              "container2"
            ],
            "readOnly": true,
            "daysBeforeDeletion": 365,
            "allowBlobPublicAccess": false
          }
        ]
      },
      "extraAppSettings": {
        "value": {
          "key1": "value1",
          "key2": "<secret>secret_name</secret>"
        }
      },
      "cosmosContainers": {
        "value": [
          {
            "name": "containerName",
            "partitionKey": "/partitionKey",
            "uniqueKeys": [
              "/uniqueKey"
            ]
          }
        ]
      },
      "deploymentSlots": {
        "value": [
          {
            "name": "dev-bis"
          }
        ]
      },
      "contributionGroups": {
        "value": [
          {
            "id": "XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX"
          }
        ]
      }
    }
  }